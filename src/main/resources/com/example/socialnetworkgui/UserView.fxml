            <columns>
                <TableColumn fx:id="columnFriendFirstName" prefWidth="98.66665649414062" text="First Name" />
                <TableColumn fx:id="columnFriendLastName" prefWidth="100.0" text="Last Name" />
                <TableColumn fx:id="columnFriendEmail" prefWidth="165.33333333333331" text="Email" />
            </columns>
        </TableView>
        <Button layoutX="67.0" layoutY="331.0" mnemonicParsing="false" onAction="#onAddFriendClick" prefHeight="25.0" prefWidth="103.0" text="Add friend" />
        <Button layoutX="192.0" layoutY="331.0" mnemonicParsing="false" onAction="#onDeleteFriendClick" prefHeight="25.0" prefWidth="103.0" text="Delete friend" />
        <TextField fx:id="textSearchByName" layoutX="268.0" layoutY="55.0" prefHeight="25.0" prefWidth="169.0" />
