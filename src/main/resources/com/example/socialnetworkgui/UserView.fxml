        <Label fx:id="errorLabelText" layoutX="118.0" layoutY="314.0" prefHeight="17.0" prefWidth="365.0" />
        <Button layoutX="432.0" layoutY="331.0" mnemonicParsing="false" onAction="#onFriendRequestsClick" prefHeight="25.0" prefWidth="103.0" text="Friend Requests" />
        <Button fx:id="btnMessages" layoutX="509.0" layoutY="200.0" mnemonicParsing="false" onAction="#onShowMessagesClick" text="Messages" />
