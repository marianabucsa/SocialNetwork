<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.socialnetworkgui.controller.UserUsersController"
            prefHeight="476.0" prefWidth="564.0">
    <children>
        <Button layoutX="438.0" layoutY="22.0" mnemonicParsing="false" onAction="#onSearchClick" prefHeight="25.0"
                prefWidth="112.0" text="Search"/>
        <Label layoutX="14.0" layoutY="26.0" prefHeight="17.0" prefWidth="65.0" text="Search user"/>
        <ScrollPane fx:id="usersScrollPane" layoutX="14.0" layoutY="131.0" prefHeight="337.0" prefWidth="536.0">
            <content>
                <VBox fx:id="usersVBox" prefHeight="328.0" prefWidth="527.0"/>
            </content>
        </ScrollPane>
        <TextField fx:id="textSearchByName" layoutX="96.0" layoutY="22.0" prefHeight="25.0" prefWidth="321.0"/>
        <Button layoutX="14.0" layoutY="87.0" mnemonicParsing="false" onAction="#onShowFriendsClick" prefHeight="25.0"
                prefWidth="112.0" text="Show friends"/>
        <Button layoutX="438.0" layoutY="87.0" mnemonicParsing="false" onAction="#onSentRequests" prefHeight="25.0"
                prefWidth="112.0" text="Sent requests"/>
        <Button layoutX="226.0" layoutY="87.0" mnemonicParsing="false" onAction="#onReceivedRequestsClick"
                text="Received requests"/>
    </children>
</AnchorPane>
